{
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/image-uploader", "dest": "/src/index.ts" },
    { "src": "/api/image-uploader/search/:str?", "dest": "/src/index.ts" },
    { "src": "/api/image-uploader/add", "dest": "/src/index.ts" },
    { "src": "/api/image-uploader/edit", "dest": "/src/index.ts" },
    { "src": "/api/image-uploader/delete/:id", "dest": "/src/index.ts" },
    { "src": "/", "dest": "/src/index.ts" }
  ]
}
